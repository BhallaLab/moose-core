create BinomialRng b
showfield b *
setfield b n 15
setfield b p 0.3
showfield b *

create ExponentialRng e
showfield e *
setfield e mean 1.0
showfield e *

create GammaRng g
showfield g *
setfield g alpha 1.0
setfield g theta 1.0
showfield g *

create NormalRng n
showfield n *
setfield n mean 0.0
setfield n variance 1.0
showfield n *

create PoissonRng p
showfield p *
setfield p mean 10
showfield p *
